    "Type": "Talk",
    "WaitingType": "Click",
    "NextStepID": "lbp_phase2_0181",
    "TalkData": {
      "SpecificSpeaker": "Maid",
      "UseBranchIndex": true,
      "Text": "ええ、もちろん入れるわ……一方的に苛められる気持ちを、貴方にも分からせてあげる。ふふ、そんなに悪いものではないから……安心して？",
      "VoiceData": {
        "Sadist": {
          "VoiceFile": "S6_Y_107988.ogg",
          "IsChoppingAudio": false
        }
      }
    }
  },

  "lbp_postyotogi_0020_T_Sadist": {
