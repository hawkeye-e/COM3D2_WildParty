  "lbp_postyotogi_0060_T_Sadist": {
    "ID": "lbp_postyotogi_0060_T_Sadist",
    "Type": "Talk",
    "WaitingType": "Click",
    "NextStepID": "lbp_postyotogi_0061_Branch",
    "TalkData": {
      "SpecificSpeaker": "Maid",
      "UseBranchIndex": true,
      "Text": "案外、私達ってお似合いなのかもしれないわね？　そう思っているのが、私だけじゃないといいけど……",
      "VoiceData": {
        "Sadist": {
          "VoiceFile": "S6_Y_107868.ogg",
          "IsChoppingAudio": false
