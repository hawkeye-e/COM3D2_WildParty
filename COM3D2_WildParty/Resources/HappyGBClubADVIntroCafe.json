    "TalkData": {
      "SpecificSpeaker": "NPC_M",
      "Index": 0,
      "Text": "もし[=Name,NPC_F,2,CallName]がチョーカーをつけていたら、あの二人はそんな風に話しかけることはルール違反だ。"
    }
  },

  "hgbc_intro_cafe_0065": {
    "ID": "hgbc_intro_cafe_0065",
    "Type": "Talk",
